}//populateHentaiTitleStatusView

function populateFansubs() {
  $.get('/api/fansubs', function(object, status) {
    if (object.data.length < 1) {
      return;
    }

    $.each(object.data, function (i, item) {
      $('#hentai_titles_search_fansubs').append($('<option>', {
          value: item.id,
